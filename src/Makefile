include Makefile.cincludes

top=../
ede_FILES=Project.ede Makefile

lisp_SOURCES=lisp.c lval.c mpc.c eval.c string-helpers.c
lisp_OBJ= lisp.o lval.o mpc.o eval.o string-helpers.o
CC=gcc
C_COMPILE=$(CC) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
C_DEPENDENCIES=-Wp,-MD,.deps/$(*F).P
C_LINK=$(CC) $(CFLAGS) $(LDFLAGS) -L.
LDDEPS= -lm -ledit
VERSION=1.1
DISTDIR=$(top)Simple Lisp-$(VERSION)/src
top_builddir = ..

DEP_FILES=.deps/lisp.P .deps/lval.P .deps/mpc.P .deps/eval.P .deps/string-helpers.P .deps/errors.P

all: lisp

DEPS_MAGIC := $(shell mkdir .deps > /dev/null 2>&1 || :)
-include $(DEP_FILES)

%.o: %.c
	@echo '$(C_COMPILE) -c $<'; \
	$(C_COMPILE) $(C_DEPENDENCIES) -o $@ -c $<

lisp: $(lisp_OBJ)
	$(C_LINK) -o $@ $^ $(LDDEPS)

tags: 


clean:
	rm -f *.mod *.o *.obj .deps/*.P .lo

.PHONY: dist

dist:
	mkdir $(DISTDIR)
	cp $(lisp_SOURCES) $(ede_FILES) $(DISTDIR)

Makefile: Project.ede
	@echo Makefile is out of date!  It needs to be regenerated by EDE.
